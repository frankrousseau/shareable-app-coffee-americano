{
  "name": "my-bookmarks-client",
  "version": "0.0.1",
  "description": "Client for My Bookmarks application.",
  "main": "src/index.coffee",
  "dependencies": {
    "react": "^0.13.1",
    "slug": "^0.8.0",
    "superagent": "^1.1.0"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
